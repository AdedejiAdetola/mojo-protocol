{
  "version": 2,
  "github": {
    "enabled": false
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/stats/:net",
      "destination": "/api/stats/:net"
    },
    {
      "source": "/stats/v2/:net",
      "destination": "/api/stats/v2/:net"
    }
  ]
}
